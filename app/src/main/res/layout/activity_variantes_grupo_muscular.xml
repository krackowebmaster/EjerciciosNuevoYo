<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/btnRegresar"
            android:layout_width="34dp"
            android:layout_height="38dp"
            android:layout_weight="1"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/btn_regresar_3x" />
        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:text="Variantes por\ngrupo muscular"
            android:textColor="@color/nuevoyo"
            android:textSize="22sp"
            android:textStyle="bold"
            android:layout_marginTop="10dp"
            android:layout_weight="8"
            />
        <ImageView
            android:id="@+id/btnHome"
            android:layout_width="34dp"
            android:layout_height="38dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/btn_mihome"
            android:layout_weight="1"/>
    </LinearLayout>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guiaMedia"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5"
        />
    <Button
        android:id="@+id/btnPectorales"
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:textSize="14sp"
        android:padding="1dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="#009fda"
        android:fontFamily="sans-serif"
        android:text="Pectorales"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:textStyle="bold"
        app:backgroundTint="#4994d1"
        app:layout_constraintVertical_bias="0.65"
        app:layout_constraintBottom_toBottomOf="@+id/btnEspalda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnEspalda"
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:textSize="14sp"
        android:padding="1dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="#009fda"
        android:fontFamily="sans-serif"
        android:text="Espalda alta (dorsales)"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:textStyle="bold"
        app:backgroundTint="#4994d1"
        app:layout_constraintVertical_bias="0.15"
        app:layout_constraintBottom_toBottomOf="@+id/btnMuslos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPectorales" />

    <Button
        android:id="@+id/btnMuslos"
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:textSize="14sp"
        android:padding="1dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="#009fda"
        android:fontFamily="sans-serif"
        android:text="Muslos"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:textStyle="bold"
        app:backgroundTint="#4994d1"
        app:layout_constraintVertical_bias="0.15"
        app:layout_constraintBottom_toBottomOf="@+id/btnPantorrilla"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnEspalda" />

    <Button
        android:id="@+id/btnPantorrilla"
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:textSize="14sp"
        android:padding="1dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="#009fda"
        android:fontFamily="sans-serif"
        android:text="Pantorrillas"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:textStyle="bold"
        app:backgroundTint="#4994d1"
        app:layout_constraintVertical_bias="0.15"
        app:layout_constraintBottom_toBottomOf="@+id/btnEspaldaBaja"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnMuslos" />

    <Button
        android:id="@+id/btnEspaldaBaja"
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:textSize="14sp"
        android:padding="1dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="#009fda"
        android:fontFamily="sans-serif"
        android:text="Espalda baja (lumbar)"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:textStyle="bold"
        app:backgroundTint="#4994d1"
        app:layout_constraintVertical_bias="0.15"
        app:layout_constraintBottom_toBottomOf="@+id/btnAbdomen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnPantorrilla" />

    <Button
        android:id="@+id/btnAbdomen"
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:textSize="14sp"
        android:padding="1dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="#009fda"
        android:fontFamily="sans-serif"
        android:text="Abdomen"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:textStyle="bold"
        app:backgroundTint="#4994d1"
        app:layout_constraintVertical_bias="0.15"
        app:layout_constraintBottom_toBottomOf="@+id/btnHombros"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnEspaldaBaja" />

    <Button
        android:id="@+id/btnHombros"
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:textSize="14sp"
        android:padding="1dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="#009fda"
        android:fontFamily="sans-serif"
        android:text="Hombros"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:textStyle="bold"
        app:backgroundTint="#4994d1"
        app:layout_constraintVertical_bias="0.15"
        app:layout_constraintBottom_toBottomOf="@+id/btnBiceps"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnAbdomen" />

    <Button
        android:id="@+id/btnBiceps"
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:textSize="14sp"
        android:padding="1dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="#009fda"
        android:fontFamily="sans-serif"
        android:text="Bíceps"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:textStyle="bold"
        app:backgroundTint="#4994d1"
        app:layout_constraintVertical_bias="0.15"
        app:layout_constraintBottom_toBottomOf="@+id/btnTriceps"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnHombros" />

    <Button
        android:id="@+id/btnTriceps"
        android:layout_width="match_parent"
        android:layout_height="34dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/rounded_button"
        android:backgroundTint="#009fda"
        android:fontFamily="sans-serif"
        android:padding="1dp"
        android:text="Tríceps"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:textSize="14sp"
        android:textStyle="bold"
        app:backgroundTint="#4994d1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnBiceps" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="90dp"
        android:layout_height="77dp"
        android:layout_marginEnd="120dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guiaMedia"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnTriceps"
        app:layout_constraintVertical_bias="0.9"
        app:srcCompat="@drawable/logo_novo_3x" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif"
        android:text="No. Reg. Sanitario: 096M2019 SSA \nNo. de Aviso: 213300202C2987\nCódigo Interno: MX21OZM00027\nPublicidad exclusiva para profesionales de la salud"
        android:textColor="#77787b"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/btnTriceps"
        app:layout_constraintVertical_bias="0.854" />


</androidx.constraintlayout.widget.ConstraintLayout>